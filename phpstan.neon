includes:
  - vendor/jangregor/phpstan-prophecy/extension.neon

parameters:
  level: 5

  paths:
    - ext_emconf.php
    - ext_localconf.php
    - Classes
  excludes_analyse:
    - Classes/Mail/Mailer.php
    - Classes/Mail/FluidEmail.php

  ignoreErrors:
    - '#Variable \$_EXTKEY might not be defined.#'
  reportUnmatchedIgnoredErrors: false
